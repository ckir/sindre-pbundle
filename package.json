{
  "name": "sindre-p",
  "version": "1.0.0",
  "description": "All Promise Utils functions from github.com/sindresorhus bundled together for use in browser!",
  "main": "build/sindre-p.js",
  "type": "module",
  "scripts": {
    "build": "rm -Rf dist build && rollup -c",
    "postinstall": "patch-package"
  },
  "keywords": [
    "promise",
    "promises",
    "helper",
    "helpers",
    "util",
    "utils",
    "sindresorhus",
    "async",
    "await",
    "async/await"
  ],
  "author": "Costas Kirgoussios",
  "license": "MIT",
  "dependencies": {
    "delay": "*",
    "hard-rejection": "*",
    "loud-rejection": "*",
    "p-all": "*",
    "p-any": "*",
    "p-break": "*",
    "p-cancelable": "*",
    "p-catch-if": "*",
    "p-debounce": "*",
    "p-defer": "*",
    "p-do-whilst": "*",
    "p-each-series": "*",
    "p-event": "*",
    "p-every": "*",
    "p-filter": "*",
    "p-forever": "*",
    "p-if": "*",
    "p-immediate": "*",
    "p-is-promise": "*",
    "p-lazy": "*",
    "p-limit": "*",
    "p-locate": "*",
    "p-log": "*",
    "p-map": "*",
    "p-map-series": "*",
    "p-memoize": "*",
    "p-min-delay": "*",
    "p-one": "*",
    "p-pipe": "*",
    "p-progress": "*",
    "p-props": "*",
    "p-queue": "*",
    "p-race": "*",
    "p-reduce": "*",
    "p-reflect": "*",
    "p-retry": "*",
    "p-series": "*",
    "p-settle": "*",
    "p-some": "*",
    "p-tap": "*",
    "p-throttle": "*",
    "p-time": "*",
    "p-timeout": "*",
    "p-times": "*",
    "p-try": "*",
    "p-wait-for": "*",
    "p-waterfall": "*",
    "p-whilst": "*",
    "pify": "*"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/node": "^20.11.14",
    "rollup": "^3.29.4",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-ts": "^3.4.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
